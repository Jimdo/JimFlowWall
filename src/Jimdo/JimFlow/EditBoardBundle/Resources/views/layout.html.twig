<html>
    <head>
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js" type="text/javascript"></script>
        <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.8.18/jquery-ui.min.js"></script>
        <link href="http://fonts.googleapis.com/css?family=Roboto:100,400,700" rel="stylesheet" type="text/css">
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
        {% javascripts
            '@JimdoJimFlowEditBoardBundle/Resources/public/js/*'
        %}
            <script src="{{'//' ~  app.request.host ~ asset_url }}"></script>
        {% endjavascripts %}

        {% stylesheets filter='cssrewrite'
            '@JimdoJimFlowEditBoardBundle/Resources/public/css/*'
        %}
            <link href="{{ '//' ~  app.request.host ~ asset_url }}" type="text/css" rel="stylesheet" />
        {% endstylesheets %}

    </head>
    <body>
        <div id="header">
        </div>
        <div id="content">
            <img src="{{ asset('bundles/jimdojimfloweditboard/images/jimflow-logo.png') }} " alt = "jimkanban_logo"/>

            <div>
                 {% block body %} {% endblock %}
            </div>
        </div>
        <div id="footer">
            <a href="http://www.it-agile.de" target="_blank" id="agilelogo"><img src="{{ asset('bundles/jimdojimflowviewboard/images/logo-itagile.png') }}"/></a>
            <a href="http://www.jimdo.com" target="_blank" id="jimdologo"><img src="{{ asset('bundles/jimdojimflowviewboard/images/logo-jimdo.png') }}"/></a>
        </div>
