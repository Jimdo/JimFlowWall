{
    "require": {
        "php":              ">=5.3.2",
        "symfony/symfony":  ">=2.0.10,<2.1.0-dev",
        "doctrine/orm":     ">=2.1.0,<2.2.0-dev",
        "twig/extensions":  "master-dev",
        "symfony/assetic-bundle":         "2.0.*-dev",
        "kriswallsmith/buzz":             "dev-master",
        "sensio/generator-bundle":        "2.0.*-dev",
        "sensio/framework-extra-bundle":  "2.0.*-dev",
        "sensio/distribution-bundle":     "2.0.*-dev",
        "jms/security-extra-bundle":      "1.0.*-dev",
        "schnipseljagd/testdatabuilder":  "dev-master",
        "zf2": "master-dev",
        "knplabs/knp-snappy": "master-dev"
    },

    "repositories": [
           {
                "type": "package",
                "package": {
                    "version": "master",
                    "name": "zf2",
                    "source": {
                        "type": "git",
                        "url": "http://github.com/zendframework/zf2.git",
                        "reference": "master"
                    },
                    "autoload": {
                        "psr-0": {
                            "Zend\\": "library/"
                        }
                    }
                }
           }
       ],

    "autoload": {
        "psr-0": {
            "Jimdo": "src/"
        }
    }
}
